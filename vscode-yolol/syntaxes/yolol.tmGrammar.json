{
	"scopeName": "yolol",
	"patterns": [
		{
			"include": "#expression"
		}
	],
	"repository": {
		"expression": {
			"patterns": [
				{
					"include": "#keyword"
				},
				{
					"include": "#keyword2"
				},
				{
					"include": "#string"
				},
				{
					"include": "#comment"
				},
				{
					"include": "#constant"
				},
				{
					"include": "#function"
				},
				{
					"include": "#operator"
				},
				{
					"include": "#extvariable"
				},
				{
					"include": "#variable"
				}
			]
		},
		"keyword": {
			"match": "(?i)(?<=^|\\s|[^a-zA-Z0-9_:])(not|abs|sqrt|sin|cos|tan|asin|acos|atan)(?=[^a-zA-Z0-9_:]|$)",
			"name": "keyword.control"
		},
		"keyword2": {
			"match": "(?i)(?<=^|\\s|[^a-zA-Z_:])(if|then|else|end|goto)+",
			"name": "keyword.control"
		},
		"string": {
			"match": "\"[^\"]*\"",
			"name": "string"
		},
		"constant": {
			"match": "[0-9]+(\\.[0-9]+)?",
			"name": "constant.numeric"
		},
		"function": {
			"match": "([a-zA-Z]+)\\(",
			"captures": {
				"1": {
					"name": "support.function"
				}
			}
		},
		"operator": {
			"match": "(?i)(?<=[^a-zA-Z0-9_:])(and|or)(?=[^a-zA-Z0-9_:])",
			"name": "keyword.operator"
		},
		"extvariable": {
			"match": ":[a-zA-Z0-9_:]+",
			"name": "variable.language"
		},
		"variable": {
			"match": "[a-zA-Z]+[a-zA-Z0-9_]*",
			"name": "variable"
		},
		"comment": {
			"begin": "//",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.comment.go.mod"
				}
			},
			"end": "$",
			"name": "comment.line"
		}
	}
}